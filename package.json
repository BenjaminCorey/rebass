{
  "name": "rebass",
  "version": "0.3.3",
  "description": "Configurable React Stateless Functional UI Components",
  "author": "Brent Jackson",
  "license": "MIT",
  "scripts": {
    "yuidoc": "yuidoc -p src",
    "prepublish": "mkdir -p dist && babel src --out-dir dist --ignore __tests__",
    "static": "static-react docs/components/Root.js --props docs/data.js > build/index.html",
    "build": "mkdir -p build && mkdir -p build/demo && npm run static && npm run demo && npm run copy",
    "copy": "cp demo/bundle.js build/demo && cp demo/index.html build/demo",
    "demo": "webpack -p --progress --colors --config demo/webpack.config.js",
    "demo:dev": "webpack-dev-server --progress --colors --config demo/webpack.config.js",
    "deploy": "./deploy.sh",
    "watch": "npm run start && onchange 'src/**/*' 'docs/**/*' -- npm run start",
    "fix": "standard --fix",
    "test": "standard && jest"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "enzyme": "^2.4.1",
    "gh-pages": "^0.11.0",
    "gridsys": "^1.0.0-beta.1",
    "jest": "^15.1.1",
    "json-loader": "^0.5.4",
    "jsonp": "^0.2.0",
    "jsx-to-string": "^0.2.16",
    "object-assign": "^4.1.0",
    "onchange": "^3.0.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-component-permutations": "^1.0.0-beta2",
    "react-docgen": "^2.10.0",
    "react-dom": "^15.3.1",
    "react-element-to-jsx-string": "^4.1.0",
    "react-geomicons": "^2.1.0",
    "react-router": "^2.8.0",
    "reflexbox": "^2.1.0",
    "standard": "^8.0.0",
    "static-react": "^3.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "yuidocjs": "^0.10.2"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "understyle": "^1.3.0"
  },
  "keywords": [
    "components",
    "react",
    "react-component",
    "ui",
    "presentational",
    "stateless",
    "functional"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/rebass.git"
  },
  "bugs": {
    "url": "https://github.com/jxnblk/rebass/issues"
  },
  "homepage": "https://github.com/jxnblk/rebass",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "docs/**/*",
      "demo/**/*"
    ],
    "globals": [
      "test",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "context"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
